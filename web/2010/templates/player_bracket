[% BLOCK column_data %]
	[%- i = start -%]
	[%- WHILE i < end %]
		[%- game = "game_$i" %][% j = i + 1 %][% other_game = "game_$j" %][% factor = j / 2 %][% next = factor + 32 -%]
		<tr>
			<td class = "[% class %]" height = "[% height %]" valign = "center">
			[% picks.$game %]
		  	&nbsp;&nbsp;<a href ="javascript:popUpPieGraph('/jqmcbp/graphs/game_[% next %].html')"><img height = 15 src='/jqmcbp/images/bullet.gif' border=0></a>
		  	<a href ="javascript:popUpPieGraph('/jqmcbp/graphs/chimp_game_[% next %].html')"><img height = 15 src='/johnnyquest/images/chimp.gif' border=0></a>
			<br>
		 	[% picks.$other_game %]
		  	</td>
		</tr>
	[% i = i + 2 %]
	[% END -%]
[% END %]
