[% IF keyword == 'top' %]


<script type="text/javascript">
	function startIt() {
		var austDay = new Date();
		austDay = new Date([% year %], 02, 12, 20);
		$('#defaultCountdown').countdown({until: austDay});
		$('#year').text(austDay.getFullYear());
	}

	$(document).ready(
	function() {

		var stage1 = 2200;
		var stage2 = 10800;
		var stage3 = 11300;
		var stage4 = 18200;
		var stage5 = 20800;

		setTimeout(function() {
			$( '#slogan' ).airport( [ 'RETURN TO GREATNESS' ] );
			$( '#rejoice' ).airport( [ 'Rejoice' ] );
		}, stage1);

		setTimeout(function() {
			$('#strikeme').addClass('strikeme');
			$( '#hidemetext').fadeOut();
			//$( '#footer').fadeOut();
		}, stage2);

		setTimeout(function() {
			$("#typeme").typewriter();
			$("#typeme").show();
		}, stage3);

		setTimeout(function() {
			$( '#showmetext').fadeIn();
		}, stage4);

		setTimeout(function() {
			$("#defaultCountdown").fadeIn('slow');
			$("#stayTuned").fadeIn('slow');
			startIt();
			$( '#footer').fadeIn('slow');
		}, stage5);
	}
	);
</script>

[% ELSIF keyword == 'enter' || keyword == 'overview' || keyword == 'willtherebeajqmcbpthisyear' %]
<script type="text/javascript">
	function startIt() {
		var austDay = new Date();
		austDay = new Date([% year %], 02, 12, 20);
		$('#defaultCountdown').countdown({until: austDay});
		$('#year').text(austDay.getFullYear());
